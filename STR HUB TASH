createBangToggle("Face Bang v2 | Ø£Ù…Ø§Ù…ÙŠ v2", 1.5, true)

Tab6 = Window:MakeTab({"Ø§Ù„Ø³ÙƒØ±Ø¨ØªØ§Øª Ø§Ù„Ø¬Ø§Ù‡Ø²Ø©","rbxassetid://114769879906735"})
function safeLoad(urlOrCode, isRawCode)
ok, err = pcall(function()
if isRawCode then loadstring(urlOrCode)()
else loadstring(game:HttpGet(urlOrCode))() end
end)
if not ok then print("Error loading script: "..tostring(err)) end
end

