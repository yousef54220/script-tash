local LoadingConfig = {
    Duration = 8,
    FadeOutTime = 1.5,
    BarColor = Color3.fromRGB(80, 40, 20), -- Ø£Ø²Ø±Ù‚ ØºØ§Ù…Ù‚
    BarGlow = Color3.fromRGB(80, 40, 20), -- Ø£Ø²Ø±Ù‚ ÙØ§ØªØ­
    BackgroundColor = Color3.fromRGB(0, 0, 0),
    TextColor = Color3.fromRGB(255, 255, 255),
    ParticleCount = 150,
    ParticleSpeed = 2,
    ParticleSize = 3
}

local LoadingElements = {
