local TextSave = ""
local Speed = 0.5
local IsSpamming = false
local SpammingConnection
local tcs = game:GetService("TextChatService")
local chat = tcs.ChatInputBarConfiguration.TargetTextChannel

function sendchat(msg)
if (tcs.ChatVersion == Enum.ChatVersion.LegacyChatService) then
game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents"):FindFirstChild("SayMessageRequest"):FireServer(msg,"All")
else
chat:SendAsync(msg)
end
end

