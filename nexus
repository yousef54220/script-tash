    Name = "ÿ≥ÿ®ÿßŸÖ ÿ¥ÿßÿ™",
    Default = false,
    Flag = "Text Spawning",
    Callback = function(Value)
        getgenv().ChaosHubSpawnText = Value
        while getgenv().ChaosHubSpawnText do
            sendchat(TextSave)
            task.wait(getgenv().ChaosHubSendDelay)
        end
    end
})

